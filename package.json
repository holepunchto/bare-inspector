{
  "name": "bare-inspector",
  "version": "5.0.0",
  "description": "V8 inspector support for Bare",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./console": {
      "types": "./lib/console.d.ts",
      "default": "./lib/console.js"
    },
    "./session": {
      "types": "./lib/session.d.ts",
      "default": "./lib/session.js"
    },
    "./server": {
      "types": "./lib/server.d.ts",
      "default": "./lib/server.js"
    },
    "./heap-snapshot": {
      "types": "./lib/heap-snapshot.d.ts",
      "default": "./lib/heap-snapshot.js"
    },
    "./package": "./package.json"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "binding.c",
    "binding.js",
    "lib",
    "CMakeLists.txt",
    "prebuilds"
  ],
  "addon": true,
  "scripts": {
    "test": "prettier . --check && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-inspector.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-inspector/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-inspector#readme",
  "engines": {
    "bare": ">=1.2.0"
  },
  "dependencies": {
    "bare-events": "^2.1.0",
    "bare-http1": "^4.0.0",
    "bare-stream": "^2.0.0",
    "bare-url": "^2.0.0",
    "bare-ws": "^2.0.0"
  },
  "devDependencies": {
    "bare-tcp": "^2.0.3",
    "brittle": "^3.3.2",
    "cmake-bare": "^1.1.6",
    "prettier": "^3.4.1",
    "prettier-config-standard": "^7.0.0"
  },
  "peerDependencies": {
    "bare-tcp": "*"
  },
  "peerDependenciesMeta": {
    "bare-tcp": {
      "optional": true
    }
  }
}
